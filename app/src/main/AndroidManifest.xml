<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:dist="http://schemas.android.com/apk/distribution"
        package="com.mlsoluciones.prenatal">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <dist:module dist:instant="true" />

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".CalculoEdadGestacionalActivity"></activity>
        <activity android:name=".ActivityExamenes" />
        <activity android:name=".ActivityPerfil" />
        <activity android:name=".ActivitySubirEccobtreticaTransvaginal" />
        <activity android:name=".ActivitySubirEcografiaTranslucenciaNucal" />
        <activity android:name=".ActivitySubirEcografiaDopplerArteriaUterinas" />
        <activity android:name=".EcografiaActivity" />
        <activity
                android:name=".HomeTabActivity"
                android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ActivityHome2" />
        <activity android:name=".ActivityLogin" />
        <activity android:name=".RegistrateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts" />
    </application>

</manifest>